switch(FEAT_ROADVEHS, SELF, mbactros2540_graphics_open_bulk_spritestack0, 
	[STORE_TEMP(CB_FLAG_MORE_SPRITES | PALETTE_USE_DEFAULT, 0x100)]
)
{
	mbactros2540_open_empty;
}

switch(FEAT_ROADVEHS, SELF, mbactros2540_graphics_open_bulk_spritestack1, 
	[STORE_TEMP(LOAD_TEMP(0), 0x100),
	(cargo_classes & bitmask(CC_COVERED)) == bitmask(CC_COVERED)])
{
	1: mbactros2540_open_bulk_tarpaulin_group;
	mbactros2540_open_bulk_group;
}

// extra_callback_info1 contains iteration of sprite stacking
// 0: draw vehicle as 2cc
// 1: draw bulk cargo
switch(FEAT_ROADVEHS, SELF, mbactros2540_graphics_open_bulk_spritestack, 
	[STORE_TEMP(cb_colour_bulk_freight(),0),
	 getbits(extra_callback_info1, 8, 8)])
{
	1: mbactros2540_graphics_open_bulk_spritestack1;
	mbactros2540_graphics_open_bulk_spritestack0;
}

// truck has 8t capacity, trailer has 16t
switch(FEAT_ROADVEHS, SELF, mbactros2540_cb_cargo_capacity, position_in_consist) {
	0: return 8;
	return 16;
}

switch(FEAT_ROADVEHS, SELF, mbactros2540_switch_graphics_open, position_in_consist) {
	0: mbactros2540_graphics_open_bulk_spritestack;
	trailer_mbactros2540_graphics_open_bulk_spritestack;
}

switch(FEAT_ROADVEHS, SELF, mbactros2540_open_cb_articulation, extra_callback_info1) {
	1: return ID_49_mbactros2540_open;
	return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

// open
item(FEAT_ROADVEHS, ID_49_mbactros2540_open) {
	property{
		name: string(TRUCK_MB_ACTROS_2540_OPEN);
		introduction_date:date(1996,01,01);
		reliability_decay:18;
		vehicle_life:10;
        model_life:20;
        loading_speed:6;
        climates_available: bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPIC);
        speed:80 km/h;
        running_cost_factor:7;
        running_cost_base:RUNNING_COST_ROADVEH;
        sprite_id:SPRITE_ID_NEW_ROADVEH;
        cargo_capacity:8;
		default_cargo_type:COAL;
		cost_factor:4;
		refit_cost:0;
		sound_effect:SOUND_DEPARTURE_OLD_RV_2;
		length:8;
        power:400 hp;
        weight:8 tons;
        tractive_effort_coefficient:0.3;
        air_drag_coefficient: 0;
        retire_early:7;
        misc_flags: bitmask(ROADVEH_FLAG_2CC, ROADVEH_FLAG_AUTOREFIT, ROADVEH_FLAG_SPRITE_STACK);
		refittable_cargo_classes:bitmask(CC_BULK);
		non_refittable_cargo_classes:bitmask(CC_HAZARDOUS, CC_REFRIGERATED);
		variant_group: ID_49_mbactros2540_tarpaulin;
	}
	graphics {
		default: mbactros2540_switch_graphics_open;
		additional_text: return string(STR_PURCHASE_MENU2, string(STR_CFG_TRUCK), string(STR_TRUCK_AUTOREFIT));
		cargo_capacity: mbactros2540_cb_cargo_capacity;
	}
}

